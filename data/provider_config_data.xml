<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Default seed providers for quick setup -->
        <record id="provider_claude_haiku" model="ai.analyst.provider.config">
            <field name="name">Claude Haiku (Cheap)</field>
            <field name="sequence">10</field>
            <field name="provider_type">anthropic</field>
            <field name="model_name">claude-3-5-haiku-latest</field>
            <field name="temperature">0.1</field>
            <field name="max_tokens">4096</field>
            <field name="timeout_seconds">60</field>
            <field name="max_retries">2</field>
            <field name="is_active" eval="True"/>
            <field name="is_default" eval="False"/>
            <field name="api_key_param">env:ANTHROPIC_API_KEY</field>
        </record>

        <record id="provider_claude_sonnet" model="ai.analyst.provider.config">
            <field name="name">Claude Sonnet (Default)</field>
            <field name="sequence">20</field>
            <field name="provider_type">anthropic</field>
            <field name="model_name">claude-sonnet-4-6-latest</field>
            <field name="temperature">0.1</field>
            <field name="max_tokens">4096</field>
            <field name="timeout_seconds">60</field>
            <field name="max_retries">2</field>
            <field name="is_active" eval="True"/>
            <field name="is_default" eval="True"/>
            <field name="api_key_param">env:ANTHROPIC_API_KEY</field>
        </record>

        <record id="provider_claude_opus" model="ai.analyst.provider.config">
            <field name="name">Claude Opus (Premium)</field>
            <field name="sequence">30</field>
            <field name="provider_type">anthropic</field>
            <field name="model_name">claude-opus-4-6-latest</field>
            <field name="temperature">0.1</field>
            <field name="max_tokens">8192</field>
            <field name="timeout_seconds">90</field>
            <field name="max_retries">2</field>
            <field name="is_active" eval="True"/>
            <field name="is_default" eval="False"/>
            <field name="api_key_param">env:ANTHROPIC_API_KEY</field>
            <field name="fallback_provider_id" ref="provider_claude_sonnet"/>
        </record>

        <record id="provider_openai_default" model="ai.analyst.provider.config">
            <field name="name">OpenAI GPT-4o</field>
            <field name="sequence">40</field>
            <field name="provider_type">openai</field>
            <field name="model_name">gpt-4o</field>
            <field name="temperature">0.1</field>
            <field name="max_tokens">4096</field>
            <field name="timeout_seconds">60</field>
            <field name="max_retries">2</field>
            <field name="is_active" eval="True"/>
            <field name="is_default" eval="False"/>
            <field name="api_key_param">env:OPENAI_API_KEY</field>
        </record>

    </data>
</odoo>
