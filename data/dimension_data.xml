<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Gender - embedded in product name -->
        <record id="ai_dimension_gender" model="ai.analyst.dimension">
            <field name="name">Gender</field>
            <field name="code">gender</field>
            <field name="model_name">sale.order.line</field>
            <!-- FIXED: was product_id.product_tmpl_id.name, now uses JSONB name field -->
            <field name="field_name">product_id.product_tmpl_id.name</field>
            <field name="sequence">10</field>
        </record>

        <!-- Age Group - embedded in product name -->
        <record id="ai_dimension_age_group" model="ai.analyst.dimension">
            <field name="name">Age Group</field>
            <field name="code">age_group</field>
            <field name="model_name">sale.order.line</field>
            <!-- FIXED: was product_id.product_tmpl_id.name, now uses JSONB name field -->
            <field name="field_name">product_id.product_tmpl_id.name</field>
            <field name="sequence">20</field>
        </record>

        <!-- Brand - embedded in default_code prefix -->
        <record id="ai_dimension_brand" model="ai.analyst.dimension">
            <field name="name">Brand</field>
            <field name="code">brand</field>
            <field name="model_name">sale.order.line</field>
            <!-- FIXED: was product_id.product_tmpl_id.name, now default_code for brand code extraction -->
            <field name="field_name">product_id.default_code</field>
            <field name="sequence">30</field>
        </record>

        <!-- Category - using real category names from DB -->
        <record id="ai_dimension_category" model="ai.analyst.dimension">
            <field name="name">Category</field>
            <field name="code">category</field>
            <field name="model_name">sale.order.line</field>
            <field name="field_name">product_id.categ_id.name</field>
            <field name="sequence">40</field>
        </record>

        <!-- Season - FIXED: stored in default_code, not product_tag -->
        <record id="ai_dimension_season" model="ai.analyst.dimension">
            <field name="name">Season</field>
            <field name="code">season</field>
            <field name="model_name">sale.order.line</field>
            <!-- FIXED: was product_id.product_tmpl_id.product_tag_ids.name, now default_code contains SS25/FW25/AW25 -->
            <field name="field_name">product_id.default_code</field>
            <field name="sequence">50</field>
        </record>

        <!-- Color - embedded in product name -->
        <record id="ai_dimension_color" model="ai.analyst.dimension">
            <field name="name">Color</field>
            <field name="code">color</field>
            <field name="model_name">sale.order.line</field>
            <field name="field_name">product_id.product_tmpl_id.name</field>
            <field name="sequence">60</field>
        </record>

