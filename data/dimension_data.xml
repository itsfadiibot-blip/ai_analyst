<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Gender - embedded in product name -->
        <record id="ai_dimension_gender" model="ai.analyst.dimension">
            <field name="name">Gender</field>
            <field name="code">gender</field>
            <field name="model_name">sale.order.line</field>
            <field name="field_name">product_id.product_tmpl_id.name</field>
            <field name="sequence">10</field>
        </record>

        <!-- Age Group - embedded in product name -->
        <record id="ai_dimension_age_group" model="ai.analyst.dimension">
            <field name="name">Age Group</field>
            <field name="code">age_group</field>
            <field name="model_name">sale.order.line</field>
            <field name="field_name">product_id.product_tmpl_id.name</field>
            <field name="sequence">20</field>
        </record>

        <!-- FIXED: Brand stored in x_studio_many2one_field_mG9Pn → x_brand -->
        <record id="ai_dimension_brand" model="ai.analyst.dimension">
            <field name="name">Brand</field>
            <field name="code">brand</field>
            <field name="model_name">sale.order.line</field>
            <!-- FIXED: was product_id.default_code, now uses real brand field -->
            <field name="field_name">product_id.product_tmpl_id.x_studio_many2one_field_mG9Pn.name</field>
            <field name="sequence">30</field>
        </record>

        <!-- FIXED: Category stored in x_sfcc_primary_category (NOT categ_id which is always "All") -->
        <record id="ai_dimension_category" model="ai.analyst.dimension">
            <field name="name">Category</field>
            <field name="code">category</field>
            <field name="model_name">sale.order.line</field>
            <!-- FIXED: was product_id.categ_id.name (always "All"), now uses SFCC category -->
            <field name="field_name">product_id.product_tmpl_id.x_sfcc_primary_category</field>
            <field name="sequence">40</field>
        </record>

        <!-- FIXED: Season stored in x_studio_many2many_field_IXz60 → x_product_tags -->
        <record id="ai_dimension_season" model="ai.analyst.dimension">
            <field name="name">Season</field>
            <field name="code">season</field>
            <field name="model_name">sale.order.line</field>
            <!-- FIXED: was product_tag_ids or default_code, now uses real custom season field -->
            <field name="field_name">product_id.product_tmpl_id.x_studio_many2many_field_IXz60.name</field>
            <field name="sequence">50</field>
        </record>

        <!-- Color - embedded in product name -->
        <record id="ai_dimension_color" model="ai.analyst.dimension">
            <field name="name">Color</field>
            <field name="code">color</field>
            <field name="model_name">sale.order.line</field>
            <field name="field_name">product_id.product_tmpl_id.name</field>
            <field name="sequence">60</field>
        </record>

        <!-- Gender Synonyms -->
        <record id="ai_syn_gender_men" model="ai.analyst.dimension.synonym">
            <field name="dimension_id" ref="ai_dimension_gender"/>
            <field name="synonym">men</field>
