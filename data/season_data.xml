        <!-- FIXED: Seasons are stored in x_product_tags table, NOT in default_code -->
        <!-- These patterns match exact tag names in x_product_tags -->

        <!-- FW25 - Fall/Winter 2025 -->
        <record id="ai_season_fw25" model="ai.analyst.season.config">
